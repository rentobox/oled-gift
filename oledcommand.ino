#define GIF_WIDTH       128
#define GIF_HEIGHT      64
#define GIF_FRAMES      epd_bitmap_allArray
#define GIF_FRAME_COUNT epd_bitmap_allArray_LEN

uint8_t       gifFrame       = 0;
unsigned long lastFrameTime  = 0;
const unsigned long FRAME_INTERVAL_MS = 80;   // speed of animation

unsigned long lastClockUpdate = 0;
const unsigned long CLOCK_REFRESH_MS = 500;   // how often to redraw clock

// === FORWARD DECLARATIONS ===
void connectWiFi();
void setupTime();
long daysSinceAnniversary();
void drawClockScreen();
void updateGif();


// === SETUP ===


void setup() {
    Serial.begin(115200);
    delay(200);

    pinMode(BUTTON_PIN, INPUT_PULLUP);

    Wire.begin(SDA_PIN, SCL_PIN);

    if (!oledOne.begin(SSD1306_SWITCHCAPVCC, OLED1_ADDR)) {
        Serial.println("oledOne allocation failed");
        while (1) { delay(10); }
    }
    if (!oledTwo.begin(SSD1306_SWITCHCAPVCC, OLED2_ADDR)) {
        Serial.println("oledTwo allocation failed");
        while (1) { delay(10); }
    }

    oledOne.clearDisplay();
    oledOne.setTextSize(1);
    oledOne.setTextColor(SSD1306_WHITE);
    oledOne.setCursor(0, 0);
    oledOne.println("Connecting WiFi...");
    oledOne.display();

    connectWiFi();
    setupTime();

    // Small splash, then main loop handles clock + GIF
    oledOne.clearDisplay();
    oledOne.setCursor(0, 0);
    oledOne.println("Clock ready");
    oledOne.display();
    delay(500);
}


//  === MAIN LOOP ===
void loop() {
  unsigned long now = millis();

  // Update GIF on OLED2
  if (now - lastFrameTime >= FRAME_INTERVAL_MS) {
    lastFrameTime = now;
    updateGif();
  }

  // Update clock + days on OLED1
  if (now - lastClockUpdate >= CLOCK_REFRESH_MS) {
    lastClockUpdate = now;
    drawClockScreen();
  }
}

// === WIFI / TIME HELPERS ===
void connectWIFI(){
	WIFI.mode(WIFI_STA);
	WIFI.begin(WIFI_SSID, WIFI_PASS); // called from prior lines

	Serial.print("Connecting to WIFI");
	while (WIFI.status() != WL_CONNECTED) {
		delay(500);
		Serial.print(".") // this will be loading dots.
	}
	Serial.println("\nWIFI connected!") // will skip a line and print once connected. 
}

void setupTime(){
	configTime(GMT_OFFSET_SEC, DST_OFFSET_SEC, "pool.ntp.org", "time.nist.gov");
	Serial.println("Waiting for time...")
	while (!getLocalTime(&timeinfo)) {
		delay(500);
		Serial.print(".") // this will be loading dots.
}
Serial.println("\nTime synced!")
}

// === CLOCK / ANNIVERSARY ===
long daysSinceAnniversary() {
  struct tm nowTm;
  if (!getLocalTime(&nowTm)) {
    return 0;
  }

  struct tm startTm = {0};
  startTm.tm_year = START_YEAR - 1900;
  startTm.tm_mon  = START_MONTH - 1;
  startTm.tm_mday = START_DAY;

  time_t now_t   = mktime(&nowTm);
  time_t start_t = mktime(&startTm);

  if (now_t < start_t) return 0;

  long days = (long)((now_t - start_t) / 86400);
  return days;
}

void drawClockScreen() {
  struct tm timeinfo;
  if (!getLocalTime(&timeinfo)) {
    return;
  }

  char timeStr[9];   // "HH:MM:SS"
  char dateStr[16];  // "Mon dd yyyy"

  // 12-hour: "%I:%M:%S" ; 24-hour: "%H:%M:%S"
  strftime(timeStr, sizeof(timeStr), "%I:%M:%S", &timeinfo);
  strftime(dateStr, sizeof(dateStr), "%b %d %Y", &timeinfo);

  long days = daysSinceAnniversary();

  oledOne.clearDisplay();

  oledOne.setTextColor(SSD1306_WHITE);

  // Time
  oledOne.setTextSize(2);
  oledOne.setCursor(0, 0);
  oledOne.println(timeStr);

  // Date
  oledOne.setTextSize(1);
  oledOne.setCursor(0, 24);
  oledOne.println(dateStr);

  // Days since anniversary
  oledOne.setCursor(0, 40);
  oledOne.print("Days together:");
  oledOne.setCursor(0, 52);
  oledOne.print(days);

  oledOne.display();
}

// === GIF UPDATE (OLED2) ===
void updateGif() {
  // Advance frame index
  gifFrame++;
  if (gifFrame >= GIF_FRAME_COUNT) {
    gifFrame = 0;
  }

  oledTwo.clearDisplay();
  oledTwo.drawBitmap(
    0, 0,
    GIF_FRAMES[gifFrame],   // epd_bitmap_allArray[gifFrame]
    GIF_WIDTH,
    GIF_HEIGHT,
    SSD1306_WHITE
  );
  oledTwo.display();
}

